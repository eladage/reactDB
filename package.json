{
  "name": "test_react",
  "version": "1.0.0",
  "description": "react playground",
  "main": "main.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "electron": "concurrently \"npm run server\" \"npm run client\" \"electron .\"",
    "just-electron": "electron .",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=assets/app-icon/win/app.ico"
  },
  "author": "Eric Ladage",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "cors": "^2.8.4",
    "electron": "^3.0.13",
    "electron-packager": "^13.0.1",
    "express": "^4.16.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "^1.1.5",
    "tedious": "^2.6.4"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eladage/reactDB.git"
  },
  "bugs": {
    "url": "https://github.com/eladage/reactDB/issues"
  },
  "homepage": "https://github.com/eladage/reactDB#readme"
}
